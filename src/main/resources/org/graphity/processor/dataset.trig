@prefix rdf:	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:	<http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:	<http://www.w3.org/2001/XMLSchema#> .
@prefix ldt:	<http://www.w3.org/ns/ldt#> .
@prefix core:	<http://www.w3.org/ns/ldt/core#> .
@prefix dh:	<http://www.w3.org/ns/ldt/document-hierarchy#> .
@prefix dct:	<http://purl.org/dc/terms/> .
@prefix void:	<http://rdfs.org/ns/void#> .
@prefix sioc:	<http://rdfs.org/sioc/ns#> .
@prefix foaf:	<http://xmlns.com/foaf/0.1/> .
@prefix gc:     <http://graphity.org/gc#> .
@prefix gps:	<http://graphity.org/gps#> .

<> a dh:Container ;
    dct:title "Graphity Processor" ;
    dct:description "Generic Linked Data processor" ;
    foaf:maker <http://atomgraph.com/#company> ;
    rdfs:seeAlso gps: , <sparql>, <http://atomgraph.com>, <http://github.com/AtomGraph> .

<sparql> a core:SPARQLEndpoint ;
    sioc:has_space <> ;
    dct:title "SPARQL endpoint" .

<service> a core:GraphStore ;
    sioc:has_space <> ;
    dct:title "Graph Store Protocol endpoint" .

# containers

<ontologies/> a dh:Container ;
    dh:slug "ontologies" ;
    sioc:has_parent <> ;
    dct:title "Ontologies" .

<classes/> a dh:Container ;
    dh:slug "classes" ;
    sioc:has_parent <> ;
    dct:title "Classes" .

<templates/> a dh:Container ;
    dh:slug "templates" ;
    sioc:has_parent <> ;
    dct:title "Templates" .

<queries/> a dh:Container ;
    dh:slug "queries" ;
    sioc:has_parent <> ;
    dct:title "Queries" .

<query-templates/> a dh:Container ;
    dh:slug "query-templates" ;
    sioc:has_parent <> ;
    dct:title "Query templates" .

<resources/labelled/> a dh:Container ;
    dh:slug "labelled" ;
    sioc:has_parent <resources/> ;
    dct:title "Labelled resources" .

# ontologies 

<ontologies/linked-data-templates> a dh:Document ;
    dh:slug "linked-data-templates" ;
    sioc:has_container <ontologies/> ;
    dct:title "Graphity Processor ontology document" ;
    foaf:primaryTopic ldt: .

<ontologies/sp> a dh:Document ;
    dh:slug "sp" ;
    sioc:has_container <ontologies/> ;
    dct:title "SP ontology document" ;
    foaf:primaryTopic <http://spinrdf.org/sp#> .

<ontologies/spin> a dh:Document ;
    dh:slug "spin" ;
    sioc:has_container <ontologies/> ;
    dct:title "SPIN ontology document" ;
    foaf:primaryTopic <http://spinrdf.org/spin#> .

<ontologies/sioc> a dh:Document ;
    dh:slug "sioc" ;
    sioc:has_container <ontologies/> ;
    dct:title "SIOC ontology document" ;
    foaf:primaryTopic sioc: .

<ontologies/foaf> a dh:Document ;
    dh:slug "foaf" ;
    sioc:has_container <ontologies/> ;
    dct:title "FOAF ontology document" ;
    foaf:primaryTopic foaf: .

<ontologies/void> a dh:Document ;
    dh:slug "void" ;
    sioc:has_container <ontologies/> ;
    dct:title "VoID ontology document" ;
    foaf:primaryTopic void: .

# templates

<templates/template-container> a dh:Document ;
    dh:slug "template-container" ;
    sioc:has_container <templates/> ;
    foaf:primaryTopic gps:TemplateContainer .

<templates/template-item> a dh:Document ;
    dh:slug "template-item" ;
    sioc:has_container <templates/> ;
    foaf:primaryTopic gps:TemplateItem .

<templates/query-container> a dh:Document ;
    dh:slug "query-container" ;
    sioc:has_container <templates/> ;
    foaf:primaryTopic gps:QueryContainer .

<templates/query-item> a dh:Document ;
    dh:slug "query-item" ;
    sioc:has_container <templates/> ;
    foaf:primaryTopic gps:QueryItem .

<templates/label-resources-container> a dh:Document ;
    dh:slug "label-resources-container" ;
    sioc:has_container <templates/> ;
    foaf:primaryTopic gps:LabelResourcesContainer .

# queries

<queries/template-item-constructor> a dh:Document ;
    dh:slug "template-item-constructor" ;
    sioc:has_container <queries/> ;
    foaf:primaryTopic gps:TemplateItemConstructor .

<queries/query-item-constructor> a dh:Document ;
    dh:slug "query-item-constructor" ;
    sioc:has_container <queries/> ;
    foaf:primaryTopic gps:QueryItemConstructor .

<queries/describe-graph-label-resources> a dh:Document ;
    dh:slug "describe-graph-label-resources" ;
    sioc:has_container <queries/> ;
    foaf:primaryTopic gps:DescribeLabelResources .

# constraints

<constraints/missing-rdfs-label> a dh:Document ;
    dh:slug "missing-rdfs-label" ;
    sioc:has_container <constraints/> ;
    foaf:primaryTopic gps:MissingRDFSLabel .

<constraints/missing-rdfs-is-defined-by> a dh:Document ;
    dh:slug "missing-rdfs-is-defined-by" ;
    sioc:has_container <constraints/> ;
    foaf:primaryTopic gps:MissingRDFSisDefinedBy .

# query templates

<query-templates/missing-property-value> a dh:Document ;
    dh:slug "missing-property-value" ;
    sioc:has_container <query-templates/> ;
